{
  "name": "vue2-framework-lint",
  "version": "0.1.2",
  "private": true,
  "scripts": {
    "setup": "yarn && yarn postinstall",
    "postinstall": "husky install",
    "serve": "http_env=serve vue-cli-service serve",
    "test": "cross-env http_env=test vue-cli-service build --mode test",
